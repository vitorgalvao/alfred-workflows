				<string>readonly plist_file='com.vitorgalvao.launchd.recentdownloadscacheadded.plist'
readonly launchagents_dir="${HOME}/Library/LaunchAgents"
readonly installed_plist_file="${launchagents_dir}/${plist_file}"
[[ -d "${launchagents_dir}" ]] || mkdir -p "${launchagents_dir}"

readonly action="${1}"

function install_agent {
  cp "${plist_file}" "${installed_plist_file}"

  # Set correct directory to watch
  # Current method might not be robust enough
  # if there are characters in the path which need escaping
  readonly expanded_dir="${downloads_dir/#\~/${HOME}}"
  sed -i '' "s|DOWNLOADS_DIR|${expanded_dir}|" "${installed_plist_file}"

  launchctl bootstrap "gui/$(id -u "${USER}")" "${installed_plist_file}"
}

function uninstall_agent {
  launchctl bootout "gui/$(id -u "${USER}")" "${installed_plist_file}"
  rm "${installed_plist_file}"
}

if [[ "${action}" == 'install' ]]; then
  install_agent
else
  uninstall_agent
fi</string>
