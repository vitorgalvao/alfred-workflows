				<string>function notification {
  /bin/zsh ./Notificator.app/Contents/Resources/Scripts/notificator --message "${1}" --title "${alfred_workflow_name}"
}

function get_detail {
  local detail repo

  repo="${1}"
  detail="${2}"

  curl --silent "https://api.github.com/repos/${repo}/releases/latest" | grep "${detail}" | head -1 | awk '{print $2}' | tr -d \",
}

readonly app_repo='vitorgalvao/pinplus'
readonly app_name='PinPlus'
readonly dest_dir='/Applications'
readonly app_dest="${dest_dir}/${app_name}.app"

readonly upstream_version="$(get_detail "${app_repo}" 'tag_name')"
readonly local_version="$(mdls -name kMDItemVersion -raw "${app_dest}")"

if [[ "${upstream_version}" == "${local_version}" ]]; then
  notification "${app_name} app is already at latest version"
  exit 0
fi

notification "Downloading ${app_name} ${upstream_version}…"
readonly url="$(get_detail "${app_repo}" 'browser_download_url')"

if curl --silent --location --no-buffer "${url}" | ditto -xk - "${dest_dir}"; then
  notification "Updated ${app_name} app to ${upstream_version}"
else
  notification "Failed to update ${app_name} app"
fi</string>
