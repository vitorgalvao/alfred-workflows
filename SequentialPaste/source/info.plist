				<string>readonly count_file="${alfred_workflow_cache}/counts_file.txt"
mkdir -p "${alfred_workflow_cache}"

readonly default_reset='2'
[[ "${reset_mins}" =~ ^[0-9]+$ ]] || reset_mins="${default_reset}"
[[ "${reset_mins}" -lt '1' ]] &amp;&amp; reset_mins='1'

if [[ "${1}" == 'reset' ]]; then
  rm "${count_file}"
  echo -n 'reset'
  exit 0
fi

readonly count="$(
  if [[ ! -f "${count_file}" || "$(find "${count_file}" -mmin +"${reset_mins}")" ]]; then
    echo '0'
  else
    bc &lt;&lt;&lt; "$(&lt; "${count_file}") + 1"
  fi
)"

echo "${count}" &gt; "${count_file}"
echo -n  "${count}"</string>
