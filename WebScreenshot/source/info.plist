				<string>require 'json'

url_list = "#{ENV['alfred_workflow_cache']}/urls.txt"

unless File.exist?(url_list)
  puts({items: [{ title: 'No URLs to list', valid: false }] }.to_json)
  abort
end

script_filter_items = []

file_contents = File.open(url_list).read
file_contents.each_line.with_index do |line, index|
  screenshot_url = line.split('|')[0].strip
  screenshot_del = line.split('|')[1].strip
  screenshot_name = File.basename(screenshot_url)

  script_filter_items.push(
    title: "#{index + 1} / #{screenshot_name}",
    subtitle: '↵ to open in web browser, ⌘↵ to delete, ⌥↵ to copy, ⇧ to view in quicklook',
    mods: {
        cmd: { subtitle: 'Open screenshot deletion page', arg: screenshot_del },
        alt: { subtitle: 'Copy screenshot link' }
    },
    arg: screenshot_url
  )
end

puts({ items: script_filter_items }.to_json)
